{
    "schemaVersion": 1,
    "id": "mc_cat_mod",
    "version": "${version}",
    "name": "MC Cat Mod",
    "description": "Giving cats the utility and armor they deserve.",
    "authors": [
        "Me!"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "CC0-1.0",
    "icon": "assets/mc_cat_mod/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.example.CatMod"
        ],
        "client": [
            "com.example.CatModClient"
        ]
    },
    "mixins": [
        "mc_cat_mod.mixins.json",
        {
            "config": "mc_cat_mod.client.mixins.json",
            "environment": "client"
        }
    ],
    "depends": {
        "fabricloader": ">=0.18.2",
        "minecraft": "~1.21.11",
        "java": ">=21",
        "fabric-api": "*"
    }
}